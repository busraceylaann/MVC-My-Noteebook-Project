@using projemm3.Entities
@model Note
<div class="row">
    <div class="col-md-12">
        <h3>
            @Model.Title
        </h3>
    </div>
    <div class="col-md-12">
        <hr />
    </div>
    <div class="col-md-12">
        <span class="glyphicon glyphicon-user"></span>@Model.Owner.Username<br />
        <span class="glyphicon glyphicon-time"></span>@Model.ModifiedOn<br />
    </div>
    <div class="col-md-12">
        <hr />
    </div>
    <div class="col-md-12">
       <p class="text-justify">
           @Model.Text
       </p>
    </div>
</div>
    
